<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>AutoFixture.Community.ImmutableCollections</id>
    <version>1.0.0-alpha</version>
    <title>AutoFixture: Creating Immutable Collections</title>
    <authors>Søren Guldmund</authors>
    <owners>Søren Guldmund</owners>
    <projectUrl>https://github.com/Miista/AutoFixture.Community.ImmutableCollections</projectUrl>
    <repository type="git" url="https://github.com/Miista/AutoFixture.Community.ImmutableCollections.git" />
    <license type="expression">MIT</license>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>An AutoFixture customization that allows AutoFixture to create specimens of the immutable collections found in the System.Collections.Immutable namespace.</description>
    <copyright>Copyright 2020</copyright>
    <tags>AutoFixture Community Immutable ImmutableCollection ImmutableArray ImmutableList ImmutableDictionary ImmutableHashSet ImmutableQueue</tags>
    <releaseNotes>Initial release</releaseNotes>

    <dependencies>
      <group targetFramework="netstandard2.0">
        <dependency id="AutoFixture" version="4.3.0" />
        <dependency id="System.Collections.Immutable" version="1.5.0" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src="netstandard2.0\*.pdb" target="lib\netstandard2.0" />
    <file src="netstandard2.0\*.xml" target="lib\netstandard2.0" />
  </files>
</package>
